    "sinon": "^2.0.0-pre.6",
    "sinon-chai": "^2.8.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "vue-loader": "^11.1.3",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "vue": "^2.1.10",
    "vue-router": "^2.2.1",
    "vuex": "^2.1.2",
    "vuex-router-sync": "^4.1.2"
