{
  "name": "frontend-boilerplate",
  "version": "0.1.0",
  "description": "frontend project boilerplate",
  "scripts": {
    "start": "cd dist && nva-server -P . -A . --mock ../.nva/mock/*.js --index /index/",
    "test": "jest",
    "test:e2e": "nva-test-e2e -s test/e2e/server.js -c test/e2e/config.js",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "jest": {
    "testMatch": [
      "**/test/unit/spec/**/*.js"
    ],
    "moduleNameMapper": {
      "^@/(.+)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "mapCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "gulp"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "moxios": "^0.4.0",
    "nva-server": "^0.4.6",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.1.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0"
  },
  "dependencies": {
    "autobind-decorator": "^1.4.1",
    "axios": "^0.15.3",
    "font-awesome": "^4.7.0",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "redux": "^3.6.0",
    "redux-container": "^1.0.26"
  }
}
