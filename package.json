{
  "name": "frontend-boilerplate",
  "version": "0.1.0",
  "description": "frontend project boilerplate",
  "scripts": {
    "start": "nva-server --rewrites --content dist --mock ./.nva/mock/*.js",
    "test": "jest",
    "test:e2e": "nva-test-e2e -c test/e2e/config.js",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "jest": {
    "testMatch": [
      "**/test/unit/spec/**/*.js"
    ],
    "moduleNameMapper": {
      "^@/(.+)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "mapCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "mobx-react-devtools": "^5.0.1",
    "moxios": "^0.4.0",
    "nva-server": "^0.5.33",
    "react-test-renderer": "^16.3.2",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "axios": "^0.18.0",
    "font-awesome": "^4.7.0",
    "mobx": "^4.2.0",
    "mobx-container": "0.0.10",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1"
  }
}
