{
  "name": "frontend-boilerplate",
  "version": "0.1.0",
  "description": "frontend project boilerplate",
  "scripts": {
    "start": "nva-server -P dist -A dist --rewrites",
    "test": "nva test",
    "e2e": "nva test --e2e -r test/e2e/server.js",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "gulp"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-env": "^2.0.20",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "moxios": "^0.3.0",
    "nib": "^1.1.2",
