{
  "name": "frontend-boilerplate",
  "version": "0.1.0",
  "description": "frontend project boilerplate",
  "main": "gulpfile.js",
  "scripts": {
    "start": "nva-server -P dist -A dist",
    "test": "NODE_ENV=test ava --tap | tap-spec",
    "e2e": "node ./test/e2e/runner.js --config ./test/e2e/nightwatch.conf.js --env chrome",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "ava": {
    "files": [
      "test/unit/**/*.js"
    ],
    "failFast": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "./test/unit/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "gulp"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.22.0",
    "browser-env": "^2.0.20",
    "chromedriver": "^2.27.3",
    "cross-spawn": "^5.1.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "nightwatch": "^0.9.12",
    "nock": "^9.0.6",
    "nva-server": "^0.1.81",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "selenium-server": "^3.1.0",
    "sinon": "^1.17.7",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "tap-spec": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.2",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-container": "^1.0.26",
    "redux-thunk": "^2.1.0"
  }
}
