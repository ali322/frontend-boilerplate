{
  "name": "frontend-boilerplate",
  "version": "0.1.0",
  "description": "frontend project boilerplate",
  "scripts": {
    "start": "nva-server --content dist --rewrites --mock ./.nva/mock/*.js",
    "test:e2e": "nva-test-e2e -c test/e2e/config.js",
    "test": "jest",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "jest": {
    "testMatch": [
      "**/test/unit/spec/**/*.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^@/(.+)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "mapCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "gulp"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "glob": "^7.1.2",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "lodash": "^4.17.4",
    "moxios": "^0.3.0",
    "nva-server": "^0.5.33",
    "offline-plugin": "^4.8.5",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^2.0.0",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "font-awesome": "^4.7.0",
    "normalize.css": "^7.0.0",
    "vue": "^2.5.16",
    "vue-router": "^2.2.1",
    "vuex": "^2.1.2",
    "vuex-router-sync": "^4.1.2"
  }
}
