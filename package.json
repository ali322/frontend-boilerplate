    "sass-loader": "^6.0.3",
    "sinon": "^2.0.0-pre.6",
    "sinon-chai": "^2.8.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "vue-loader": "^11.1.3",
    "vue-template-compiler": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
