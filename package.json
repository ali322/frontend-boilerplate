{
  "name": "frontend-boilerplate",
  "version": "0.1.0",
  "description": "frontend project boilerplate",
  "main": "gulpfile.js",
  "scripts": {
    "start": "nva-server -P dist -A dist",
    "test": "NODE_ENV=test karma start ./test/unit/karma.conf.js",
    "e2e": "node ./test/e2e/runner.js --config ./test/e2e/nightwatch.conf.js --env chrome",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "gulp"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.22.0",
    "browser-env": "^2.0.20",
    "chai": "^3.5.0",
    "chromedriver": "^2.27.2",
    "cross-spawn": "^5.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "html-loader": "^0.4.5",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.30",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
