{
  "name": "frontend-boilerplate",
  "version": "0.0.1",
  "description": "frontend project boilerplate",
  "main": "gulpfile.js",
  "scripts": {
    "start": "NODE_ENV=develop LISTEN_PORT=2000 node ./task/develop-server.js",
    "release": "webpack --config ./task/webpack.production.js --display-error-details",
    "dll": "webpack --config ./task/webpack.dll.js --display-error-details",
    "test": "NODE_ENV=test ava --tap | tap-spec"
  },
  "ava": {
    "files": [
      "src/__tests__/**/*.js"
    ],
    "failFast": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "./src/__tests__/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ali322/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "webpack",
    "gulp"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/ali322/frontend-boilerplate#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.22.0",
    "browser-env": "^2.0.20",
    "tap-spec": "^4.1.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
