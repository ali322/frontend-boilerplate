{
  "presets": [["env", { "modules": false }], "stage-0", "react"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-async-to-generator",
    "transform-runtime",
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    },
    "test": {
      "presets": [["env", { "targets": { "node": "current" } }]]
    }
  }
}
